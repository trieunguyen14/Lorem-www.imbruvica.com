var videoIntervalId = 0;

$(document).ready(function () {

    
    var loc = window.location.pathname.replace(/\/$/, '')

    // home
    if (loc == "" || loc == "/")
    {
        $("button").each(function () {

            $(this).click(function (event) {

                if ($(this).text() == "1") {
                    ga('send', 'event', { eventCategory: 'Tab', eventAction: 'Click', eventLabel: 'Homepage_You&Isupport_Carousel1', nonInteraction: true });
                }
                else if ($(this).text() == "2") {
                    ga('send', 'event', { eventCategory: 'Tab', eventAction: 'Click', eventLabel: 'Homepage_You&Isupport_Carousel2', nonInteraction: true });
                }
            });
        });


        $("a:not('ul.cat-list a'):not(a.show-lso):not(.mob-menu a)").each(function () {
            var href = $(this).attr("href");
            if (typeof href === "undefined") {
                href = $(this).attr("data-href");
            }
            
            var target = $(this).attr("target");
            var text = $(this).text();
            $(this).click(function (event) { // when someone clicks these links
                console.log($(this));
                //event.preventDefault(); // don't open the link yet
                if (typeof href === "undefined") {
                    // Video modal has no HREF assigned
                    if ($(this).hasClass("close-reveal-modal")) {
                        ga('send', 'event', { eventCategory: 'Video', eventAction: 'Stop', eventLabel: 'Close_Video_Button_Clicked', nonInteraction: true });
                        console.log("Tracked in GA: " + "Video Close Button Clicked");
                    }

                    return;
                }
                else {
                    console.log("Tracked in GA: " + href);
                }

                if (typeof href != 'undefined' && href.indexOf("you-i-support") >= 0) {
                    if ($(this).hasClass("button-1")) {
                        ga('send', 'event', { eventCategory: 'Navigation', eventAction: 'Click', eventLabel: 'Homepage_You&Isupport_LearnMore', nonInteraction: true });
                    }
                }

                if ($(this).hasClass("button-6"))
                {
                    if ($(this).hasClass("content-toggle-1")) {
                        ga('send', 'event', { eventCategory: 'Tab', eventAction: 'Click', eventLabel: 'Homepage_WorksDifferent_OralTab', nonInteraction: true });
                    }
                    else if ($(this).hasClass("content-toggle-2")) {
                        ga('send', 'event', { eventCategory: 'Tab', eventAction: 'Click', eventLabel: 'Homepage_WorksDifferent_ChemoTab', nonInteraction: true });
                    }
                }

                if ($(this).hasClass("video-button"))
                {
                    ga('send', 'event', { eventCategory: 'Navigation', eventAction: 'Click', eventLabel: 'Homepage_WatchBillsStory', nonInteraction: true });
                    ga('send', 'event', { eventCategory: 'Video', eventAction: 'Play', eventLabel: 'BillsStory_Video_Play', nonInteraction: true });
