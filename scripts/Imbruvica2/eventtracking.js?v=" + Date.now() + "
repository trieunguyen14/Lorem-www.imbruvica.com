var vid = document.getElementById('video_player_stories');
function videoStatus(where) {
    


    if (vid != undefined && !vid.paused) {

        var len = (vid.currentTime * 100) / vid.duration
        len = Math.round(len);

        
        if (len == 25) {
            ga('send', 'event', { eventCategory: 'Video', eventAction: 'Play', eventLabel: 'BillsStory_Video_25', nonInteraction: true });
        }
        else if (len == 50) {
            ga('send', 'event', { eventCategory: 'Video', eventAction: 'Play', eventLabel: 'BillsStory_Video_50', nonInteraction: true });
        }
        else if (len == 75) {
            ga('send', 'event', { eventCategory: 'Video', eventAction: 'Play', eventLabel: 'BillsStory_Video_75', nonInteraction: true });
        }
        else if (len == 99) {
            ga('send', 'event', { eventCategory: 'Video', eventAction: 'Play', eventLabel: 'BillsStory_Video_100', nonInteraction: true });
        }
    }
}


function videoStatusHome() {
    videoStatus("home");
}


function videoStatusCll() {
    videoStatus("cll");
}

function registerYouandIEvents() {
    $("a:not(a.show-lso):not(.mob-menu a)").each(function () {
        var href = $(this).attr("href");
        if (typeof href === "undefined") {
            href = $(this).attr("data-href");
        }
        if (typeof href === "undefined") return;

        var target = $(this).attr("target");
        var text = $(this).text();
        $(this).click(function (event) { // when someone clicks these links

            if (href.indexOf("register") >= 0) {
                if ($(this).hasClass("access")) {
                    ga('send', 'event', { eventCategory: 'Enroll', eventAction: 'Click', eventLabel: 'You&I_AccessSupport_Enroll', nonInteraction: true });
                }
                else if ($(this).hasClass("support")) {
                    ga('send', 'event', { eventCategory: 'Enroll', eventAction: 'Click', eventLabel: 'You&I_AffordabilitySupport_Enroll', nonInteraction: true });
                }
                else if ($(this).hasClass("nurse")) {
                    ga('send', 'event', { eventCategory: 'Enroll', eventAction: 'Click', eventLabel: 'You&I_NurseCallSupport_Enroll', nonInteraction: true });
                }
            }
            else if (href.indexOf("jjpaf.org") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'You&I_AffordabilitySupport_JJPAF', nonInteraction: true });
            }
        });
    });



    $(".show-lso").each(function () {

        var href = $(this).attr("href");
        if (typeof href === "undefined") {
            href = $(this).attr("data-href");
        }
        if (typeof href === "undefined") return;

        $(this).click(function (event) {

            if (href.indexOf("www.lls.org") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'You&I_AffordabilitySupport_LLS', nonInteraction: true });
            }
            else if (href.indexOf("panfoundation.org") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'You&I_AffordabilitySupport_PAN', nonInteraction: true });
            }
        });
    });

}


function reRegisterCllEvents()
{
    $(".show-lso").each(function () {

        var href = $(this).attr("href");
        if (typeof href === "undefined") {
            href = $(this).attr("data-href");
        }
        if (typeof href === "undefined") return;

        $(this).click(function (event) {

            if (href.indexOf("www.lls.org") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'CLL_Resources_LLSLink', nonInteraction: true });
            }
            else if (href.indexOf("www.cancer.org") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'CLL_Resources_Cancerorg', nonInteraction: true });
            }
            else if (href.indexOf("www.lymphoma.org") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'CLL_Resources_lymphomaorg', nonInteraction: true });
            }
            else if (href.indexOf("patientadvocate.org") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'CLL_Resources_patientadvocateorg', nonInteraction: true });
            }
            else if (href.indexOf("advocacyconnector.com") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'CLL_Resources_advocacyconnectororg', nonInteraction: true });
            }
            else if (href.indexOf("www.cancercare.org") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'CLL_Resources_cancercareorg', nonInteraction: true });
            }
            else if (href.indexOf("www.cancersupportcommunity.com") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'CLL_Resources_cancersupportcommunity', nonInteraction: true });
            }
            else if (href.indexOf("www.nccn.org/patients") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'CLL_Resources_nccnorg', nonInteraction: true });
            }
            else if (href.indexOf("mhm.care4today.com") >= 0) {
                ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'CLL_TakingIMB_Care4TodayLink', nonInteraction: true });
            }
        });
    });

}