    // Run global on other pages.
    if (loc != "" || loc != "/") {
        //global tracking only; does not track side nav links, lso interstitials, or very top nav links. 
        $("a:not('ul.cat-list a'):not(a.show-lso):not(.mob-menu a)").each(function () {
            var href = $(this).attr("href");
            if (typeof href === "undefined") {
                href = $(this).attr("data-href");
            }
            var target = $(this).attr("target");
            var text = $(this).text();
            $(this).click(function (event) { // when someone clicks these links
                console.log($(this));
                //event.preventDefault(); // don't open the link yet
                if (typeof href === "undefined") {
                    // Video modal has no HREF assigned
                    if ($(this).hasClass("close-reveal-modal")) {
                        ga('send', 'event', { eventCategory: 'Video', eventAction: 'Stop', eventLabel: 'Close_Video_Button_Clicked', nonInteraction: true });
                        console.log("Tracked in GA: " + "Video Close Button Clicked");
                    }

                    return;
                }
                else {
                    console.log("Tracked in GA: " + href);
                }

                if (href == "/") {
                    if ($(this).hasClass("logo")) {
                        ga('send', 'event', { eventCategory: 'navigation', eventAction: 'Click', eventLabel: 'TopNav_Logo', nonInteraction: true });
                    }
                    else if ($(this).hasClass("footer-link")) {
                        ga('send', 'event', { eventCategory: 'navigation', eventAction: 'Click', eventLabel: 'BotNav_Home', nonInteraction: true });
                    }
                    else {
                        ga('send', 'event', { eventCategory: 'navigation', eventAction: 'Click', eventLabel: 'TopNav_Home', nonInteraction: true });
                    }
                }


                if ($(this).hasClass("openFloatingIsi")) {
                    if ($(this).hasClass("footer-link")) {
                        if (href == "#") ga('send', 'event', { eventCategory: 'navigation', eventAction: 'Click', eventLabel: 'BotNav_ImportantSideEffects', nonInteraction: true });
                    }
                    else {
                        if (href == "#") ga('send', 'event', { eventCategory: 'navigation', eventAction: 'click', eventLabel: 'TopNav_ImportantSideEffects', nonInteraction: true });
                    }
                }


                if (href.indexOf("/prescribing-information") >= 0) {
                    ga('send', 'event', { eventCategory: 'Link', eventAction: 'click', eventLabel: 'TopNav_PI', nonInteraction: true });
                }




                if (href.indexOf("/legal") >= 0) {
                    if ($(this).hasClass("isi-link")) {
                        ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'ISI_LegalNotice', nonInteraction: true });
                    }
                    else {
                        ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'BotNav_LegalNotice', nonInteraction: true });
                    }
                }


                if (href.indexOf("www.pharmacyclics.com/privacy-policy") >= 0) {
                    if ($(this).hasClass("isi-link")) {
                        ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'ISI_PrivacyPolicy', nonInteraction: true });
                    }
                    else if ($(this).hasClass("footer-link")) {
                        ga('send', 'event', { eventCategory: 'Link', eventAction: 'Click', eventLabel: 'BotNav_PrivacyPolicy', nonInteraction: true });
                    }
                }

                //            if (href.indexOf("/important-safety-information") >= 0) ga('send', 'event', { eventCategory: 'navigation', eventAction: 'click', eventLabel: 'TopNav_ISI', nonInteraction: true });


                if (href == "/docs/librariesprovider7/default-document-library/prescribing_information.pdf?sfvrsn=2") // PI
                {
                    ga('send', 'event', { eventCategory: 'Link', eventAction: 'click', eventLabel: 'BotNav_PI', nonInteraction: true });

                }

                if (href == "/contact-us") {
                    ga('send', 'event', { eventCategory: 'navigation', eventAction: 'Click', eventLabel: 'BotNav_ContactUs', nonInteraction: true });
                }

                if (href == "/unsubscribe") {
                    ga('send', 'event', { eventCategory: 'navigation', eventAction: 'click', eventLabel: 'BotNav_UnsubscribUpdate', nonInteraction: true });
                }



                if (href == "/you-i-support") {
                    if ($(this).hasClass("button-3")) {
                        ga('send', 'event', { eventCategory: 'navigation', eventAction: 'click', eventLabel: 'TopNav_You&I_Expand_LearnMore', nonInteraction: true });
                    }
                }

                /* if (href == "/dosing") ga('send', 'event', { eventCategory: 'navigation', eventAction: 'click', eventLabel: 'TopNav_Dosing', nonInteraction: true });
                 if (href == "/sr") ga('send', 'event', { eventCategory: 'navigation', eventAction: 'click', eventLabel: 'TopNav_SupportResources', nonInteraction: true });
                 if (href == "/cll") ga('send', 'event', { eventCategory: 'navigation', eventAction: 'click', eventLabel: 'TopNav_CancerType_CLL', nonInteraction: true });
                 if (href == "/mcl") ga('send', 'event', { eventCategory: 'navigation', eventAction: 'click', eventLabel: 'TopNav_CancerType_MCL', nonInteraction: true });
                 if (href == "/wm") ga('send', 'event', { eventCategory: 'navigation', eventAction: 'click', eventLabel: 'TopNav_CancerType_WM', nonInteraction: true });
                 
                 if (href == "/cll/efficacy-data/study-design") ga('send', 'event', { eventCategory: 'navigation', eventAction: 'click', eventLabel: 'TopNav_Efficacy_StudyDesign_CLL', nonInteraction: true });
                 if (href == "/cll/efficacy-data/progression-free-survival") ga('send', 'event', { eventCategory: 'register', eventAction: 'click', eventLabel: 'TopNav_Efficacy_PFS_CLL', nonInteraction: true });
                 */

                //setTimeout(function () { // now wait 300 milliseconds...
                //    window.open(href, (!target ? "_self" : target)); // ...and open the link as usual
                //}, 300);
            });
        });
    }

    $(".leaving-ibm-hcp-ok").click(function (event) { // when someone clicks these links
        console.log($(this));
        ga('send', 'event', { eventCategory: 'link', eventAction: 'click', eventLabel: 'HCPSite_Click_OK', nonInteraction: true });
    });

    $(".leaving-ibm-hcp-cancel").click(function (event) { // when someone clicks these links
        console.log($(this));
        ga('send', 'event', { eventCategory: 'link', eventAction: 'click', eventLabel: 'HCPSite_Click_Cancel', nonInteraction: true });
    });

    


    $(".show-lso").click(function () {

        setTimeout(function () {

            $(".leaving-imb-ok").click(function (event) { // when someone clicks these links
                console.log($(this));
                ga('send', 'event', { eventCategory: 'link', eventAction: 'click', eventLabel: 'LeavingIMB_OK', nonInteraction: true });
            });

            $(".leaving-imb-cancel").click(function (event) { // when someone clicks these links
                console.log($(this));
                ga('send', 'event', { eventCategory: 'link', eventAction: 'click', eventLabel: 'LeavingIMB_Cancel', nonInteraction: true });
            });

            $("a[href='http://www.pharmacyclics.com/privacy-policy']").click(function () {

                if ($(this).hasClass("leaving-imp-pcyc")) {
                    ga('send', 'event', { eventCategory: 'link', eventAction: 'click', eventLabel: 'LeavingIMB_PCYC', nonInteraction: true });
                }
                else if ($(this).hasClass("leaving-imb")) {
                    ga('send', 'event', { eventCategory: 'link', eventAction: 'click', eventLabel: 'LeavingIMB_PrivacyPolicy', nonInteraction: true });
                }
            });


        }, 500)

        
    });

